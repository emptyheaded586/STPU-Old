<Window x:Class="Smart_Touch_Protocol_Utility.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Smart_Touch_Protocol_Utility"
        mc:Ignorable="d"
        Title="Smart Touch Protocol Utility" Height="350" Width="525" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="importButton" Content="Import" Click="importButton_Click" HorizontalAlignment="Left" Margin="154,79,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="exportButton" Content="Export" Click="exportButton_Click" HorizontalAlignment="Left" Margin="278,79,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="AddProtocolButton" Content="Add Protocol" Click="AddProtocolButton_Click" HorizontalAlignment="Left" Margin="154,104,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="ModifyButton" Content="Modify Protocol" Click="ModifyButton_Click" HorizontalAlignment="Left" Margin="278,104,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
